{
  "name": "gut-flow",
  "version": "1.0.0",
  "description": "Ein gut git Flow",
  "preferGlobal": true,
  "bin": {
    "gut": "index.js"
  },
  "scripts": {
    "lint": "eslint '**/*.js'",
    "test": "node test/cleanup.js; npm run lint; jest",
