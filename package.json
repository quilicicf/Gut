  "dependencies": {
    "base-64": "0.1.0",
    "colors": "1.3.3",
    "copy-paste-win32fix": "1.4.0",
    "inquirer": "6.2.1",
    "lodash": "4.17.11",
    "utf8": "3.0.0",
    "yargs": "12.0.2"
  },
  "devDependencies": {
    "@talend/eslint-config": "2.0.1",
    "babel-eslint": "10.0.1",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
