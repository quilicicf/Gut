{
  "extends": [
    "eslint:recommended",
    "airbnb-base/legacy"
  ],
  "env": {
    "amd": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "array-bracket-spacing": [
      2,
      "always",
      {
        "arraysInArrays": false
      }
    ]
  }
}
